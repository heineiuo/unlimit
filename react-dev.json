      "publicPath": "//unpkg.com/smile-web@0.0.8/dist/",
      "devPublicPath": "http://127.0.0.1:8083/dist/",
      "//alias": {
        "react": "react/dist/react.js",
        "react-dom": "react-dom/dist/react-dom.js"
      },
      "externals": {
        "react": {
          "root": "React",
