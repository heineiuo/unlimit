{
  "name": "gateway",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "gulp": "cd scripts; gulp",
    "start": "cd build && sudo nodemon --watch ../src --exec sudo babel-node ../src",
    "start:proxy": "cd build && nodemon --watch ../src --exec babel-node ../scripts/proxy",
    "dev-server": "sudo babel-node src/entry/server --config=build/data/config.json --nohttps=1",
    "start-server": "cd build && sudo node server/index --config=data/config.dev.json --nohttps",
    "apidoc": "apidoc -f .js -i ./src -o ./docs/apidoc",
    "build": "node scripts/webpack --build=gateway --compress",
    "build:cli": "node scripts/webpack --build=cli --compress",
    "build:debug": "node scripts/webpack --build=gateway"
  },
  "dependencies": {
    "ali-push": "^0.1.6",
    "auto-sni": "^1.5.1",
    "babel-runtime": "^6.9.2",
    "blueimp-md5": "~2.3.0",
    "body-parser": "^1.14.1",
    "chalk": "^1.1.3",
    "child-process-promise": "^2.1.3",
    "cli-table2": "^0.2.0",
    "compression": "^1.6.1",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.0",
    "decompress": "^3.0.0",
    "directory": "~0.1.0",
    "ent": "^2.2.0",
    "express": "^4.13.4",
    "formidable": "^1.0.17",
    "fs-promise": "~0.5.0",
    "homedir": "~0.6.0",
    "http-proxy": "^1.12.0",
    "joi": "~8.4.0",
    "letsencrypt-express": "^2.0.5",
    "level-sublevel": "^6.6.1",
    "leveldown": "^1.5.0",
    "levelup": "^1.3.3",
    "md5-file": "^3.1.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.6.1",
    "node-version": "^1.0.0",
    "q-level": "^1.1.3",
    "request": "^2.67.0",
    "socket.io": "^1.5.1",
    "socket.io-client": "^1.5.1",
    "ua-parser-js": "~0.7.10",
    "uuid": "~2.0.2"
  },
  "devDependencies": {
    "nedb": "^1.5.0",
    "fs-extra": "^0.26.5",
    "babel-cli": "~6.9.0",
    "babel-core": "~6.9.0",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-runtime": "~6.9.0",
    "babel-polyfill": "~6.9.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "seashell-proxy": "^0.2.4",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "~2.10.0",
    "webpack-node-externals": "^1.0.0"
  }
}
