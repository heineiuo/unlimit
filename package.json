{
  "name": "gateway-server",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "npm run web",
    "server": "cd build && sudo nodemon --watch ../src/server --exec sudo babel-node ../src/server",
    "web": "react-sh",
    "start:proxy": "cd build && nodemon --watch ../src --exec babel-node ../scripts/proxy",
    "dev-server": "sudo babel-node src/entry/server --config=build/data/config.json --nohttps=1",
    "start-server": "cd build && sudo node server/index --config=data/config.dev.json --nohttps",
    "build": "node scripts/server-dev --build --compress",
    "build:cli": "node scripts/webpack --build=cli --compress",
    "build:debug": "node scripts/webpack --build=gateway"
  },
